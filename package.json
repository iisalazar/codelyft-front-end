{
  "name": "Codelyft",
  "description": "A platform where devs can showcase their code snippets",
  "author": "Ian Salazar",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "axios": "^0.19.0",
    "concurrently": "^5.0.0",
    "electron-is-dev": "^1.1.0",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "express-http-proxy": "^1.6.0",
    "http-proxy-middleware": "^0.20.0",
    "nodemon": "^2.0.1",
    "path": "^0.12.7",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-file-drop": "^0.2.8",
    "react-highlight.js": "^1.0.7",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "uuid": "^3.3.3",
    "wait-on": "^3.3.0"
  },
  "build": {
    "appId": "com.iansalazar.codelyft",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "dev": "react-scripts start",
    "dev-server": "nodemon server.js",
    "start": "node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm run build",
    "electron-dev": "concurrently \"BROWSER=none npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder -c.extraMetadatamain=build/electron.js",
    "preelectron-pack": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dertrockx/codelyft-front-end.git"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "proxy": "http://codelift-backend.herokuapp.com"
}
